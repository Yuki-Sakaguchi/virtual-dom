{
  "name": "virtualdom",
  "version": "1.0.0",
  "description": "仮想DOMのフレームワークを作成する",
  "main": "index.js",
  "scripts": {
    "build:counter": "browserify src/ts/counter.ts -p [tsify] > dist/counter.js",
    "watch:counter": "watchify src/ts/counter.ts -p [tsify] -v --debug -o dist/counter.js",
    "build:todo": "browserify src/ts/todo.ts -p [tsify] > dist/todo.js",
    "watch:todo": "watchify src/ts/todo.ts -p [tsify] -v --debug -o dist/todo.js"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "browserify": "^16.2.3",
    "tsify": "^4.0.0",
    "typescript": "^3.1.6",
    "watchify": "^3.11.0"
  }
}
